@model AquaG.TasksMVC.ViewModels.ProjectModel

@{
    ViewData["Title"] = "Удаление проекта";
}
<h5>Удаление проекта</h5>

<p>Хотите удалить проект?</p>
<div class="text-danger" asp-validation-summary="All"></div>
<hr>
<div class="form-group">@Model.Caption</div>
<div class="form-group">@Model.Description</div>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Удалить" class="btn btn-danger" /> |
</form>
<a asp-action="Index">К списку</a>

